{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-netlify-cms": "^3.0.0-beta.4",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "netlify-cms": "^2.0.9",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.config.js -o ./src/layouts/index.css",
    "build": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "format": "prettier --write src/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.13.7",
    "purgecss": "^1.0.1",
    "tailwindcss": "^0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
